            <div class="col-lg-12 text-right">
              <button
                type="submit"
                id="submit-btn"
                class="button button-big button-dark"
              >
                SUBMIT
              </button>
              <!-- <button class="button button-big button-dark" onclick="contact_send();">SUBMIT</button> -->
            </div>
          </div>
        </form>
        <div class="social-reg text-center">
          <p>OR REGISTER USING</p>
          <div class="google">
            <a href="/auth/google">
              <span><img src="/img/google1.png" alt="google" /></span>
            </a>
            <a href="/auth/facebook">
              <span
                ><img src="/img/fb.png" alt="facebook" width="25" height="25"
              /></span>
            </a>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>






